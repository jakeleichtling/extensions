{
   "background": {
      "persistent": true,
      "scripts": [ "bg.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": ""
   },
   "description": "Quickly access crbugs you have recently viewed.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Recent Crbugs",
   "permissions": [ "webNavigation", "storage", "clipboardWrite", "https://code.google.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.1"
}
